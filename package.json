{
  "name": "nestjs-url-generator",
  "version": "0.0.7",
  "description": "NestJs library for generating url",
  "keywords": [
    "nestjs",
    "nest",
    "generating-url",
    "signed-url"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "prebuild": "rm -rf dist && rm *.tgz",
    "build": "tsc -p tsconfig.json",
    "postbuild": "npm pack && mv nestjs-url-generator-*.tgz compiled-lib.tgz",
    "update": "npm update && cd sample && npm update",
    "format": "prettier --write \"lib/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{lib,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vh13294/nestjs-url-generator.git"
  },
  "author": "Vong Panha Huot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vh13294/nestjs-url-generator/issues"
  },
  "homepage": "https://github.com/vh13294/nestjs-url-generator#readme",
  "devDependencies": {
    "@nestjs/common": "^7.5.5",
    "@nestjs/core": "^7.5.5",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^6.15.0",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "reflect-metadata": "^0.1.13",
    "@types/qs": "^6.9.0",
    "qs": "^6.9.0"
  }
}
